{
  "name": "christmas-gifts-exchange",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "Johann Rakotoharisoa",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json src/**/*.ts src/**/*.tsx",
    "clean": "rimraf dist/ && rimraf coverage/",
    "test": "mocha --opts mocha.opts",
    "coverage": "yarn run clean && tsc && istanbul cover _mocha -- dist/**/*.js",
    "cover": "nyc npm test",
    "start": "webpack-dev-server",
    "build": "npm run clean && webpack",
    "build-prod": "npm run clean && webpack -p",
    "predeploy": "npm run build-prod",
    "deploy": "gh-pages -d dist",
    "cmd": "ts-node index.ts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "node_modules/",
      "dist/",
      "src/**/*.spec.ts",
      "src/**/*.spec.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "json",
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "webpack-dev-server": "^2.4.3"
  },
  "dependencies": {
    "@types/node": "^7.0.13",
    "@types/q": "^1.0.0",
    "@types/react": "^15.0.22",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.39",
    "@types/redux": "^3.6.31",
    "@types/chai": "^3.5.1",
    "@types/core-js": "^0.9.41",
    "@types/mocha": "^2.2.41",
    "@types/node-uuid": "^0.0.28",
    "@types/sinon": "^2.1.2",
    "chai": "^3.5.0",
    "circular-dependency-plugin": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "nyc": "^10.2.0",
    "raw-loader": "^0.5.1",
    "redux-devtools": "^3.3.2",
    "sass-loader": "^6.0.3",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "ts-loader": "^2.0.3",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "bootstrap": "4.0.0-alpha.5",
    "fast-csv": "^2.4.0",
    "font-awesome": "^4.7.0",
    "node-uuid": "^1.4.8",
    "q": "^1.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "ts-node": "^3.0.2",
    "tslib": "^1.6.1"
  }
}
