{
  "name": "christmas-gifts-exchange",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "Johann Rakotoharisoa",
  "license": "MIT",
  "scripts": {
    "lint": "tslint -c tslint.json src/**/*.ts src/**/*.tsx",
    "clean": "rimraf dist/ && rimraf coverage/",
    "test": "mocha --opts mocha.opts",
    "coverage": "yarn run clean && tsc && istanbul cover _mocha -- dist/**/*.js",
    "cover": "nyc npm test",
    "start": "webpack-dev-server",
    "build": "npm run clean && webpack",
    "deploy": "gh-pages -d dist"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "node_modules/",
      "dist/"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "json",
      "html"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/core-js": "^0.9.34",
    "@types/mocha": "^2.2.32",
    "@types/node-uuid": "^0.0.28",
    "@types/sinon": "^1.16.31",
    "chai": "^3.5.0",
    "circular-dependency-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.0.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "html-webpack-plugin": "^2.24.0",
    "mocha": "^3.1.2",
    "node-sass": "^3.10.1",
    "nyc": "^8.3.2",
    "raw-loader": "^0.5.1",
    "redux-devtools": "^3.3.1",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.5",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "@types/node": "^6.0.45",
    "@types/q": "^0.0.32",
    "@types/react": "^0.14.43",
    "@types/react-dom": "^0.14.18",
    "@types/react-redux": "^4.4.32",
    "@types/redux": "^3.6.31",
    "bootstrap": "4.0.0-alpha.5",
    "fast-csv": "^2.3.0",
    "node-uuid": "^1.4.7",
    "q": "^1.4.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "ts-node": "^1.6.0"
  }
}